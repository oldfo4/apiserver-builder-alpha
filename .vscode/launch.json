    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1"
        },

        {
            "name": "apiregister-gen",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/apiregister-gen",
            "cwd": "/home/wjye/go/src/ready-go/apiserver/",
            // "env": {
            //     "GO111MODULE": "off",
            // },
            // "dlvFlags": ["--wd", "/home/wjye/go/src/ready-go/apiserver/"],
            "args": ["--input-dirs", "ready-go/apiserver/pkg/apis/...", 
            "--input-dirs", "ready-go/src/apiserver/pkg/controller/...",
            "--go-header-file", 
            "boilerplate.go.txt"],
        }
    ]
}